<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>
        <record id="model_climbing_gym_medical_certificate" model="ir.model">
            <field name="website_form_access" eval="True"/>
            <field name="website_form_label">Sales Order</field>
        </record>

        <function model="ir.model.fields" name="formbuilder_whitelist">
            <value>climbing_gym.medical_certificate</value>
            <value eval="[
                'doctor_name',
                'doctor_license',
                'issue_date',
                'certificate_image'
               ]"/>
        </function>
    </data>
</odoo>
